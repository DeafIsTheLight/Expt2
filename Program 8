#include <iostream>
#include <string>
#include <cmath>
#include <iomanip>
using namespace std;

int main (){
	int y, z, a = 0, b = 1, nVal = 0;

    cout << "Enter a number: ";
    cin >> y;
    z = y;

    cout << "Multiplication table of " << y << ":\n";

    for (int x = 1; x <= 10; ++x)
    {
        cout << z << " * " << x << " = " << (y * x) << "\n";
    }
	
}
