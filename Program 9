#include <iostream>
#include <string>
#include <cmath>
#include <iomanip>
using namespace std;

int main (){
	int a, b;
	int x, y;
	
	cout << "Width Value: " << "\n";
	cin >> y;
	
	cout << "Length Value: " << "\n";
	cin >> x;
	
	{
		
	cout << "*";
	
	for (int a = 0; a < x - 2; a++)
    {
        cout << "*";
    }
    
    cout << "*\n";

    for (int a = 0; a < y - 2; a++)
    {
        cout << "*";
        for (int b = 0; b < x - 2; b++)
        {
            cout << "*";
        }
        cout << "*\n";
    }

    cout << "*";
    
    for (int a = 0; a < x - 2; a++)
    {
        cout << "*";
    }
    
    cout << "*\n";
    
    }
}
